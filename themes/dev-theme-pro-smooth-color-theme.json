      "settings": {
        "foreground": "#bababa"
      }
    },
    {
      "name": "Sub-methods",
      "scope": ["entity.name.module", "variable.import.parameter"],
      "settings": {
        "foreground": "#578EFB"
      }
    },
    {
      "name": "entity.name.method",
      "scope": ["entity.name.method"],
      "settings": {
        "foreground": "#62F0DD"
      }
    },
    {
      "name": "meta.method",
      "scope": ["meta.class-method", "variable.function.constructor"],
      "settings": {
        "foreground": "#62F0DD"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
