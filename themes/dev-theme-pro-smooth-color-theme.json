{
  "name": "Dev Theme Pro Smooth",
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "foreground": "#D8E9FA"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.type",
        "support.class",
        "support.class.component",
        "support.other",
        "meta.use.php",
        "markup.changed.git_gutter",
        "variable.other.readwrite meta.object.member meta.objectliteral meta.object.member meta.objectliteral",
        "meta.objectliteral meta.object.member variable.other.readwrite",
        "meta.type.annotation support.type.primitive"
      ],
      "settings": {
        "foreground": "#C2B776",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Object Keys",
      "scope": ["meta.object-literal.key", "variable.other.property"],
      "settings": {
        "foreground": "#9D9ECF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "values",
      "scope": [
        "constant.numeric.decimal",
        "constant.language",
        "variable.other.constant.property",
        "constant",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#d65689"
      }
    },
    {
      "name": "Brackets",
      "scope": ["string.quoted", "string.template", "string.interpolated"],
      "settings": {
        "foreground": "#cc866a"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment", "string.quoted.docstring"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5d6969"
      }
    },
    {
      "name": "Variables + Other whites",
      "scope": [
        "variable",
        "support.variable.property",
        "meta.objectliteral meta.object.member variable.other.readwrite",
        "variable.parameter",
        "source.dart",
        "meta.var.expr meta.arrow meta.block meta.var.expr new.expr meta.objectliteral meta.object.member variable.other.readwrite"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#bababa"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#F06279"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#aa4b4b92"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "keyword.control.flow",
        "storage.type",
        "storage.modifier",
        "keyword.control.flow",
        "variable.language.this",
        "variable.language.super",
        "keyword.control.flow",
        "support.type.object.module",
        "keyword.control",
        "keyword.other",
        "punctuation.accessor.optional"
      ],
      "settings": {
        "foreground": "#A46FED",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["Keyword", "Storage"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "punctuation",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "meta.brace",
        "punctuation.definition.block",
        "meta.objectliteral punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.definition.binding-pattern.object",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#D8E9FAcc",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Simple Text",
      "scope": ["meta.jsx.children"],
      "settings": {
        "foreground": "#cbcbcb"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#e15a90"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "variable.function",
        "support.function",
        "meta.object.member.ts meta.function-call.ts entity.name.function.ts"
      ],
      "settings": {
        "foreground": "#56C98E"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "entity.name.function.ts meta.function-call.ts",
        "meta.decorator entity.name.function.ts",
        "entity.name.tag support.class.component"
      ],
      "settings": {
        "foreground": "#6194FC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "C-related Block Level Variables",
      "scope": ["source.cpp meta.block variable.other"],
      "settings": {
        "foreground": "#d65689"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#F06262"
      }
    },

    // ----------------SHELL Specific Scopes----------------

    {
      "name": "Shell - Command",
      "scope": ["source.shell"],
      "settings": {
        "foreground": "#56C98E"
      }
    },
    {
      "name": "Shell - Command",
      "scope": ["string.unquoted.argument.shell"],
      "settings": {
        "foreground": "#a8b8d7"
      }
    },
    {
      "name": "Shell - Command",
      "scope": [
        "keyword.operator.pattern.case.shell",
        "source.shell keyword.operator.pattern.case.default",
        "string.unquoted.shell",
        "punctuation.definition.variable.shell variable.other.normal.shell",
        "punctuation.definition.variable.shell",
        "punctuation.section.array.shell",
        "punctuation.definition.string.end.shell",
        "punctuation.definition.logical-expression.shell",
        "keyword.operator.pattern.case.default.shell",
        "punctuation.section.arithmetic.double.shell",
        "punctuation.section.parenthese.shell",
        "meta.parenthese.group.shell",
        "punctuation.definition.subshell.single.shell"
      ],
      "settings": {
        "foreground": "#bababa",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Shell - Command",
      "scope": ["punctuation.separator.statement.and.shell"],
      "settings": {
        "foreground": "#A46FED"
      }
    },

    // ----------------CSS / HTML / JSON Specific Scopes----------------

    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
